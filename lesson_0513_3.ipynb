    "#迴圈_while\n",
    "deposit = 0\n",
    "num = 0\n",
    "\n",
    "while (deposit < 30000):\n",
    "     num += 1\n",
    "     input_value = int(input(f\"請輸入第{num}月份存款:\"))\n",
    "     deposit += input_value\n",
    "     print(f\"共存入{deposit}，尚差{30000-deposit}元\")\n",
    "\n",
    "print(f\"存{num}月，共存{deposit}元整\")\n",
    "\n"
   ]
  }
